# CI requires branch to be defined
[submodule "rpi-usbboot"]
	path = rpi-usbboot
	url = https://github.com/raspberrypi/usbboot
	branch = master
[submodule "boot-image_firmware"]
	path = boot-image/firmware
	url = https://github.com/raspberrypi/firmware
	branch = stable
	shallow = true
[submodule "boot-image_linux"]
	path = boot-image/linux
	url = https://github.com/USA-RedDragon/linux
	branch = rpi-router
	shallow = true
[submodule "busybox"]
	path = busybox
	url = https://git.busybox.net/busybox/
	branch = refs/tags/1_35_0
[submodule "system_packages"]
	path = system/packages
	url = https://github.com/TheInternetBox/packages
	branch = main
